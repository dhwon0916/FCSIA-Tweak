{
    "manifest_version": 3,
    "name": "Mass Assign FCSIA",
    "version": "1.0",
    "description": "Add multiple students to event.",
    "permissions": [
      "activeTab", "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://event.fcsia.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  